    "vue-router": "^4.3.0",
    "vue3-markdown-it": "^1.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@playwright/test": "^1.49.0",
    "@tailwindcss/typography": "^0.5.16",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.6.1",
    "eslint": "^9.0.0",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "happy-dom": "^20.6.1",
    "pg": "^8.16.0",
